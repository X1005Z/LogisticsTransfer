.orderDetailText {
  width: 750rpx;
  height: 70rpx;
  line-height: 70rpx;
  background-color: #fde5d2;
  border-top: 2rpx solid #E7691D;
  border-bottom: 2rpx solid #E7691D;
  font-size: 28rpx;
  padding-left: 22rpx;
  box-sizing: border-box;
}

/* 收货信息 */
.orderUp {
  width: 694rpx;
  height: 358rpx;
  margin: 40rpx auto;
  padding: 30rpx 0;
  box-sizing: border-box;
  box-shadow: 0px 2px 10px 0px rgba(0, 0, 0, 0.4);
}

.warehouse {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 642rpx;
  height: 130rpx;
  margin-left: 30rpx;
  margin-bottom: 28rpx;
}

.warehouseText {
  width: 566rpx;
}

.warehouseText1 {
  font-size: 30rpx;
}

.orderText2 {
  width: 62rpx;
  height: 28rpx;
  color: #40B850;
  font-size: 24rpx;
  background-color: rgba(217, 241, 221, 100);
  margin-left: 24rpx; 
}

.warehouseImage {
  width: 52rpx;
  height: 52rpx;
  padding-top: 6rpx;
  box-sizing: border-box;
  text-align: center;
  border-radius: 50%;
}

.warehouseImage1 {
  background-color: rgba(64, 184, 80, 100);
}

.warehouseImage2 {
  background-color: #FFA726;
}

.warehouseImage image {
  width: 40rpx;
  height: 40rpx;
}

/* 订单详情 */
.fare {
  width: 694rpx;
  height: 1478rpx;
  background-color: rgba(255, 255, 255, 79);
  box-shadow: 0px 2px 10px 0px rgba(0, 0, 0, 0.4);
  margin: 0 auto;
  font-size: 28rpx;
  color: #101010;
  padding-left: 28rpx;
  padding-right: 28rpx;
  padding-top: 46rpx;
  box-sizing: border-box;
}

.fare-text {
  margin-bottom: 22rpx;
}

.fare-image {
  width: 590rpx;
  height: 164rpx;
}

.fare-image image {
  width: 186rpx;
  height: 164rpx;
  margin-right: 16rpx;
}

.fare-wrap {
  display: flex;
  width:638rpx;
  height: 240rpx;
  padding-left: 20rpx;
  margin-bottom: 28rpx;
  border: 2rpx solid #bebebe;
  border-radius: 6rpx;
  box-sizing: border-box;
}

.fare-wrap .fare-wrapLeft {
  width: 192rpx;
  height: 186rpx;
  margin-right: 26rpx;
  margin-top: 28rpx;
}

.fare-wrap .fare-wrapLeft image {
  width: 100%;
  height: 100%;
}

.fare-wrap .fare-wrapRight {
  width: 336rpx;
  height: 200rpx;
  margin-top: 22rpx;
}

.fare-wrap .fare-wrapRight .fare-wrapRightText2 {
  font-size: 24rpx;
  color: #8a858e;
}

/* 快递个数 */
.orderMiddle {
  width: 694rpx;
  height: 820rpx;
  margin: 40rpx auto;
  padding: 20rpx 0;
  box-sizing: border-box;
  box-shadow: 0px 2px 10px 0px rgba(0, 0, 0, 0.4);
}

.middleImage {
  display: flex;
}

.middleImage .images {
  width: 28rpx;
  height: 28rpx;
  margin-left: 34rpx;
}

.middleImage .texts {
  color: #EC0606;
  font-size: 24rpx;
  margin-left: 20rpx;
}

.middleText {
  display: flex;
  margin-top: 22rpx;
}

.middleText text:nth-child(1) {
  color: #EC0606;
  margin-left: 62rpx;
  font-weight: 700;
}

.middleText text:nth-child(2) {
  margin-left: 8rpx;
  color: #E7691D;
  font-weight: 700;
} 

.middleText .input {
  width: 400rpx;
  height: 52rpx;
  border: 1px solid #bbb;
  border-radius: 14rpx;
  margin-left: 16rpx;
  padding-left: 8rpx;
  box-sizing: border-box;
  font-size: 22rpx;
}

.middleBox {
  display: inline-block;
  width: 560rpx;
  height: 200rpx;
  border: 2rpx solid rgba(187, 187, 187, 100);
  border-radius: 14rpx;
  margin-top: 30rpx;
  margin-left: 72rpx;
  color: rgba(136, 136, 136, 100);
  font-size: 24rpx;
  padding-left: 10rpx;
  box-sizing: border-box;
  margin-bottom: 38rpx;
}

/* 添加快递单号 */
.upBox {
  width: 560rpx;
  height: 330rpx;
  margin: 0 auto;
}

.upBox1 {
  position: relative;
  width: 560rpx;
  height: 60rpx;
  display: flex;
  margin-bottom: 30rpx;
}

.upBox1 .upBoxText {
  width: 22rpx;
  height: 44rpx;
  text-align: center;
  margin-left: 20rpx;
  margin-top: 10rpx;
}

.upBox1 .input {
  width: 600rpx;
  height: 60rpx;
  border: 2rpx solid rgba(187, 187, 187, 100);
  border-radius: 10rpx;
  color: #888888;
  font-size: 28rpx;
  margin-left: 40rpx;
  padding-left: 10rpx;
  box-sizing: border-box;
}

.upBox1 .upBoxText1 {
  position: absolute;
  top: 15rpx;
  right: 15rpx;
  width: 70rpx;
  height: 32rpx;
  line-height: 32rpx;
  font-size: 20rpx;
  background-color: #FDE5D2;
  color: #E7691D;
  text-align: center;
  border-radius: 8rpx;
}

/* 订单详情 */
.orderUnder {
  width: 694rpx;
  height: 450rpx;
  margin: 40rpx auto;
  padding: 20rpx 0;
  box-sizing: border-box;
  box-shadow: 0px 2px 10px 0px rgba(0, 0, 0, 0.4);
}

.orderUnder .orderUnderText {
  margin-left: 30rpx;
}

.orderUnder .orderUnderText view {
  margin-bottom: 24rpx;
  font-size: 28rpx;
  color: #101010;
}


/* 按钮 */
.btnUnder {
  display: flex;
  margin-top: 116rpx;
  width: 750rpx;
  height: 114rpx;
}

.btn {
  width: 376rpx;
  height: 114rpx;
  line-height: 114rpx;
  text-align: center;
  font-size: 36rpx;
}

.btn1 {
  background-color: #E7691D;
  color: #fff;
}

.btn2 {
  background-color: #274F05;
  color: #F5E4AD;
}

/* 弹出层 */
.modal-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.5;
  overflow: hidden;
  z-index: 9000;
  color: #fff;
}

.modal-dialog {
  width: 632rpx;
  height: 364rpx;
  overflow: hidden;
  position: fixed;
  top: 50%;
  left: 0;
  z-index: 9999;
  background: #f9f9f9;
  margin: -180rpx 60rpx;
  border-radius: 16rpx;
}

.modal-title {
  color: #000000;
  font-size: 36rpx;
  padding-top: 16rpx;
  font-size: 28rpx;
  margin-bottom: 42rpx;
  text-align: center;
}

.modal-content {
  color: #888888;
  font-size: 26rpx;
  width: 520rpx;
  height: 142rpx;
  margin-left: 22rpx;
  margin-bottom: 30rpx;
}

.modal-content .modal-warehouse {
  display: flex;
  align-items: center;
}

.modal-warehouseImage {
  width: 52rpx;
  height: 52rpx;
  line-height: 60rpx;
  border-radius: 50%;
  background-color: #ffa726;
  text-align: center;
}

.modal-warehouseImage image {
  width: 34rpx;
  height: 34rpx;
}

.modal-warehouseText {
  width: 434rpx;
  height: 142rpx;
  margin-left: 34rpx;
  padding-top: 16rpx;
  color: #101010;
  box-sizing: border-box;
}

.modal-footer {
  display: flex;
  flex-direction: row;
  height: 86rpx;
  font-size: 34rpx;
  line-height: 86rpx;
  font-weight: 700;
}

.btn-cancel {
  width: 50%;
  color: #666;
  text-align: center;
}

.btn-confirm {
  width: 50%;
  color: #ec5300;
  text-align: center;
}

