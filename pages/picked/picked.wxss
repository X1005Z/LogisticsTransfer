.orderDetailText {
  width: 750rpx;
  height: 70rpx;
  line-height: 70rpx;
  background-color: #fde5d2;
  border-top: 2rpx solid #E7691D;
  border-bottom: 2rpx solid #E7691D;
  font-size: 28rpx;
  padding-left: 22rpx;
  box-sizing: border-box;
}

/* 收货信息 */
.orderUp {
  width: 694rpx;
  height: 358rpx;
  margin: 40rpx auto;
  padding: 30rpx 0;
  box-sizing: border-box;
  box-shadow: 0px 2px 10px 0px rgba(0, 0, 0, 0.4);
}

.warehouse {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 642rpx;
  height: 130rpx;
  margin-left: 30rpx;
  margin-bottom: 28rpx;
}

.warehouseText {
  width: 566rpx;
}

.warehouseText1 {
  font-size: 30rpx;
}

.orderText2 {
  width: 62rpx;
  height: 28rpx;
  color: #40B850;
  font-size: 24rpx;
  background-color: rgba(217, 241, 221, 100);
  margin-left: 24rpx; 
}

.warehouseImage {
  width: 52rpx;
  height: 52rpx;
  padding-top: 6rpx;
  box-sizing: border-box;
  text-align: center;
  border-radius: 50%;
}

.warehouseImage1 {
  background-color: rgba(64, 184, 80, 100);
}

.warehouseImage2 {
  background-color: #FFA726;
}

.warehouseImage image {
  width: 40rpx;
  height: 40rpx;
}

/* 快递个数 */
.orderMiddle {
  width: 694rpx;
  height: 800rpx;
  margin: 40rpx auto;
  padding: 20rpx 0;
  box-sizing: border-box;
  box-shadow: 0px 2px 10px 0px rgba(0, 0, 0, 0.4);
}

.middleImage {
  display: flex;
}

.middleImage .images {
  width: 28rpx;
  height: 28rpx;
  margin-left: 34rpx;
}

.middleImage .texts {
  color: #EC0606;
  font-size: 24rpx;
  margin-left: 20rpx;
}

.middleText {
  display: flex;
  margin-top: 22rpx;
}

.middleText text:nth-child(1) {
  color: #EC0606;
  margin-left: 62rpx;
  font-weight: 700;
}

.middleText text:nth-child(2) {
  margin-left: 8rpx;
  color: #E7691D;
  font-weight: 700;
} 

.middleText .input {
  width: 400rpx;
  height: 52rpx;
  border: 1px solid #bbb;
  border-radius: 14rpx;
  margin-left: 16rpx;
  padding-left: 8rpx;
  box-sizing: border-box;
  font-size: 22rpx;
}

.middleBox {
  display: inline-block;
  width: 560rpx;
  height: 200rpx;
  border: 2rpx solid rgba(187, 187, 187, 100);
  border-radius: 14rpx;
  margin-top: 30rpx;
  margin-left: 72rpx;
  color: rgba(136, 136, 136, 100);
  font-size: 24rpx;
  padding-left: 10rpx;
  box-sizing: border-box;
  margin-bottom: 38rpx;
}

/* 添加快递单号 */
.upBox {
  width: 560rpx;
  height: 330rpx;
  margin: 0 auto;
}

.upBox1 {
  position: relative;
  width: 560rpx;
  height: 60rpx;
  display: flex;
  margin-bottom: 30rpx;
}

.upBox1 .upBoxText {
  width: 22rpx;
  height: 44rpx;
  text-align: center;
  margin-left: 20rpx;
  margin-top: 10rpx;
}

.upBox1 .input {
  width: 600rpx;
  height: 60rpx;
  border: 2rpx solid rgba(187, 187, 187, 100);
  border-radius: 10rpx;
  color: #888888;
  font-size: 28rpx;
  margin-left: 40rpx;
  padding-left: 10rpx;
  box-sizing: border-box;
}

.upBox1 .upBoxText1 {
  position: absolute;
  top: 15rpx;
  right: 15rpx;
  width: 70rpx;
  height: 32rpx;
  line-height: 32rpx;
  font-size: 20rpx;
  background-color: #FDE5D2;
  color: #E7691D;
  text-align: center;
  border-radius: 8rpx;
}

/* 订单详情 */
.orderUnder {
  width: 694rpx;
  height: 344rpx;
  margin: 40rpx auto;
  padding: 20rpx 0;
  box-sizing: border-box;
  box-shadow: 0px 2px 10px 0px rgba(0, 0, 0, 0.4);
}

.orderUnder .orderUnderText {
  margin-left: 30rpx;
}

.orderUnder .orderUnderText view {
  margin-bottom: 24rpx;
  font-size: 28rpx;
  color: #101010;
}

/* 按钮 */
.btnUnder {
  margin-top: 116rpx;
  width: 750rpx;
  height: 114rpx;
  line-height: 114rpx;
  color: #fff;
  font-size: 36rpx;
  background-color: #e7691d;
  text-align: center;
}
